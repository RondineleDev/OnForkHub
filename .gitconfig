[gitflow "branch"]
    master = main
    develop = dev

[gitflow "prefix"]
    feature = feature/
    bugfix = bugfix/
    release = release/
    hotfix = hotfix/
    support = support/
    versiontag = v

[gitflow "path"]
    hooks = .husky

[gitflow "feature"]
    finish = false
    no-ff = true
    no-merge = true
    keepbranch = true
    finish-hooks = pre-flow-feature-finish